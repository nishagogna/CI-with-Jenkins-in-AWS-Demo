pipeline{
 agent any
  stages {  
  stage (build) {
    steps {
     sh '''
     export JAVA_HOME=/usr/lib/jvm/java-8-openjdk-amd64
     mvn package
     '''
    }
    }
    }
    post {
    always {
    archiveArtifacts artifacts: 'project/target/*.war', followSymlinks: false onlyIfSuccessful: true
 
  }
